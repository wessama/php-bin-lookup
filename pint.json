{
  "preset": "laravel",
  "rules": {
    "no_unused_imports": true,
    "ordered_imports": {
        "sort_algorithm": "alpha"
    },
    "concat_space": {
      "spacing": "one"
    },
    "simplified_null_return": true,
    "braces": false,
    "new_with_braces": {
      "anonymous_class": false,
      "named_class": false
    }
  }
}